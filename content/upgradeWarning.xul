<?xml version="1.0"?>
<?xml-stylesheet href="chrome://global/skin/global.css" type="text/css"?>
<dialog id="donothing" title="SASS Compiler 2.0 Update"
  xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
  buttons="cancel,accept"
  buttonlabelcancel="More info"
  buttonlabelaccept="Ok"
  ondialogcancel="window.open('https://github.com/babobski/Komodo-sass-compiler#komodo-sass-compiler', 'chrome,titlebar,toolbar');" >
    
